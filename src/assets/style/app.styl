.nav-links.el-tabs {

}

.nav-links.el-tabs .el-tabs__header {
  border: 0;
  margin: 0;
}

.nav-links.el-tabs .el-tabs__item {
  height: 53px;
  line-height: 60px;
  font-size: 16px;
}


.el-upload__input {
  display: none !important;
}

/*=========api 列表展示样式========*/
.type.post {
  color: #416bbb;
}

.type.get {
  color: #39bb2f;
}

.type.put {
  color: #bb7220;
}

.type.delete {
  color: #bb221b;
}

/*=======页面颜色==========*/

.el-table .put {
  color: put;
}

.el-table .head {
  color: head;
}

.el-table .delete {
  color: delete;
}

.el-table .post {
  color: post;
}

.el-table .patch {
  color: patch;
}

.el-table .get {
  color: get;
}

.el-table .options {
  color: options;
}

/*=========类型 列表展示样式========*/
.ia-splitter-handle {
  height: 100%;
  position: absolute;
  top: 0;
  right: -6px;
  width: 11px;
  z-index: 11;
  cursor: col-resize;
}

.ia-splitter-handle-highlight {
  background-image: url("data:image/png;base64,");
  background-position: right center;
  background-repeat: no-repeat
  position: absolute;
  top: 0;
  left: 4px;
  bottom: 0;
  width: 6px;
  border-left: 1px solid #ccc;
}

.el-radio-button__orig-radio:checked + .el-radio-button__inner {
  color: #fff;
  background-color: #1970a9;
  border-color: #1970a9;
}

.noborder input, .div-table li .noborder textarea {
  border none;
}
.el-tabs__nav-wrap::after{
  background-color: rgba(228, 231, 237, 0)
}

.my-autocomplete {
  li {
    line-height: normal;
    padding: 7px;

    .name {
      text-overflow: ellipsis;
      overflow: hidden;
    }
    .addr {
      font-size: 12px;
      color: #b4b4b4;
    }

    .highlighted .addr {
      color: #ddd;
    }
  }
}

.el-tabs--border-card {
  background: #fff;
  border: 1px solid #d1dbe5;
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, .12), 0 0 6px 0 rgba(0, 0, 0, .04)
}

.el-tabs--border-card > .el-tabs__content {
  padding: 15px
}

.el-tabs--border-card > .el-tabs__header {
  background-color: #eef1f6;
  margin: 0
}

.el-tabs--border-card > .el-tabs__header .el-tabs__item {
  transition: all .3s cubic-bezier(.645, .045, .355, 1);
  border: 1px solid transparent;
  border-top: 0;
  margin-right: -1px;
  margin-left: -1px
}

.el-tabs--border-card > .el-tabs__header .el-tabs__item.is-active {
  background-color: #fff;
  border-right-color: #d1dbe5;
  border-left-color: #d1dbe5
}

.el-tabs--border-card > .el-tabs__header .el-tabs__item.is-active:first-child {
  border-left-color: #d1dbe5
}

.el-tabs--border-card > .el-tabs__header .el-tabs__item.is-active:last-child {
  border-right-color: #d1dbe5
}
.deprecated {
    text-decoration: line-through;
}
/*==============提示==============*/
.tipWarp {
  margin 5px
}

.el-dialog__headerbtn, .el-message-box__headerbtn {
  float: right;
  background: transparent;
  border: none;
  outline: none;
  padding: 0;
  cursor: pointer;
}